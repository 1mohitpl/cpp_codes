#include<iostream>
using namespace std;
  int main(){
    int n1,n2;
    cout<<"enter the n1 and n2 number"<<" ";
    cin>>n1>>n2;
    char op;
    cout<<"input an operator";
    cin>>op;
    switch(op){
    	case'+':
    		cout<<n1+n2<<endl;
    		break;
    		case'-':
    			cout<<n1-n2<<endl;
    			break;
    			case'*':
    				cout<<n1*n2<<endl;
    				break;
    				case'/':
    					cout<<n1/n2<<endl;
    					break;
    					default:
    						cout<<"enter the another operator"<<endl;
    						break;
    						
	}
	    return 0;
 
    
  
}
 
 
